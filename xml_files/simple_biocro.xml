      <lu>/data/ed_inputs/glu/</lu>
      <thsum>/data/ed_inputs/</thsum>
    </inputs>
    -->
    
    
    
      <start.date>2004/01/01</start.date>
      <end.date>2004/12/30</end.date>
      
      <host>
        <name>localhost</name>
      </host>
      
  </run>
</pecan>

