<pecan>
  
  <database>
    <bety>
      <driver>PostgreSQL</driver>
      <user>bety</user>
      <password>bety</password>
      <host>postgres</host>
      <dbname>bety</dbname>
      <write>TRUE</write>
    </bety>
    <dbfiles>~/gsoc_project_2022/pecan_runs/pecan_run_salix/dbfiles</dbfiles>
  </database>
  
  <workflow>
    <id>1000019857</id>
  </workflow>

  <pfts>
    <pft>
      <name>salix</name>
    </pft>
  </pfts>

  <ensemble>
      <size>10</size>
      <variable>AbvGrndWood</variable>
  
      <samplingspace>
          <met>
              <method>sampling</method>
          </met>
      
          <soil>
              <parent>parameters</parent>
          </soil>
   
          <vegetation>
              <parent>soil</parent>
          </vegetation>
          
      </samplingspace>
      <ensemble.id>1000033780</ensemble.id>
 </ensemble>

  <meta.analysis>
    <iter>3000</iter>
    <random.effects>FALSE</random.effects>
    <threshold>1.2</threshold>
    <update>AUTO</update>
  </meta.analysis>

  <sensitivity.analysis>
    <quantiles>
      <sigma>-1</sigma>
      <sigma>1</sigma>
    </quantiles>
    <variable>AbvGrndWood</variable>
  </sensitivity.analysis>

  <model>
    <id>7</id>
    <binary>~/gsoc_project_2022/models/biocro.Rscript</binary>
    <revision>0.95</revision>
  </model>

  <run>
    <id>99000002926</id>
    <site>
      <id>288</id>
      <lat>31.40</lat>
      <lon>-81.37</lon>
    </site>

    <inputs>
      <met>
        <output>~/gsoc_project_2022/pecan_runs/pecan_run_salix/run</output>
        <path>~/gsoc_project_2022/data/US-Bo1</path>
      </met>
    </inputs>
    
    <!-- One way to add multiple inputs
     <inputs>
      <met>/data/sites/MERRA_ED2_site_1-42/ED_MET_DRIVER_HEADER</met>
      <veg>/data/oge2OLD/OGE2_</veg>
